{
  "name": "prep-tracker-backend",
  "version": "1.0.0",
  "description": "Software engineer interview prep tracker",
  "main": "src/index.ts",
  "author": "Aron Pereira",
  "license": "MIT",
  "scripts": {
    "start": "node dist/index.js",
    "build": "rm -rf dist && tsc",
    "dev": "nodemon --watch src --ext ts --ignore dist --exec ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage && rm -rf coverage",
    "setup-db": "ts-node scripts/setup-db.ts",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\""
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.2",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "eslint": "^10.0.2",
    "jest": "^30.2.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.8.1",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.56.1"
  },
  "dependencies": {
    "compression": "^1.8.1",
    "cors": "^2.8.6",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.3",
    "mongoose": "^8.15.0",
    "zod": "^4.3.6"
  }
}
